{
  "manifest_version": 2,
  "name": "Selective User Agent Switcher",
  "version": "1.0",
  "description": "Switches user agents for specific websites.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html"
}
